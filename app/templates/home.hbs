<section class="jumbotron text-center">
		<h1>JS Calculator</h1>
		<p>A Web Based Calculator Using Ember.JS </p>
		<article class="container">
				<div class="row">
						<div class="col-12 col-sm-6 offset-sm-3 col-lg-4 offset-lg-4">
								{{!-- <h1>Current: {{activeTheme}}</h1> --}}
								<Theme @themes={{this.model}} @activeTheme={{activeTheme}} @onChange={{action (mut activeTheme) "goToCalculator" }} />
						</div>
				</div>
		</article>
</section>